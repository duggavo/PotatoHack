{
  "schemaVersion": 1,
  "id": "potatohack",
  "version": "${version}",

  "name": "PotatoHack",
  "description": "Patate Kartoffeln!",
  "authors": [
    "duggavo"
  ],
  "contact": {
    "homepage": "https://potatohack.net/modmenu/homepage",
    "issues": "https://potatohack.net/modmenu/issues",
    "sources": "https://potatohack.net/modmenu/sources"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/potato/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "net.potatohack.PotatoInitializer"
    ]
  },
  "mixins": [
    "potato.mixins.json"
  ],
  "accessWidener" : "potato.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": ">=0.81.2",
    "minecraft": "~1.20-beta.2",
    "java": ">=17"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "wurst": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Donate": "https://potatohack.net/modmenu/donate"
      }
    }
  }
}
